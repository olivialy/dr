.clearfix
    overflow: auto

.sr-only
    position: absolute
    width: 1px
    height: 1px
    padding: 0
    margin: -1px
    overflow: hidden
    clip: rect(0, 0, 0, 0)
    border: 0

.hidden
    display: none

.float-right
    float: right

.float-left
    float: left

.align-center
    text-align: center

.content
    width: 100%
    max-width: 1170px
    margin: 0 auto
